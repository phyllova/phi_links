<script type="text/javascript">  

// -----------------   VALIDATION ------------------------------

$(document).ready(function(){
            });



	$().ready(function() {

	$('.vote').click(function(){
        var id = $(this).attr('data-id');
        var user_id = $(this).attr('data-user');
        $('#video_id').val(id);
        $('#user_id').val(user_id);

    });	

    $('#submit').click(function(e){
        var video_url = $('#video_url').val();
        if ( video_url.length < 10){
          alert('Please Upload the Video.');
          e.preventDefault();
          return false;
        }
      

    });

    $("#registeration_form").validate({

			rules: {

				name: "required",

				phone: "required",

        age: "required",

        category: "required",

        candidate_bio: "required",

        video_url: "required",

        insta_handle:"required",

				email: {

					required: true,

					email: true

				},

        password: "required",

          confirmpassword: {

            equalTo: "#password"

          }

			

			},

			messages: {

				name: "Please enter your Name",

				phone: "Please enter your Phone",

				email: "Please enter a valid email address",

				age: "Please Enter your Valid Age",

        category: "Please Select the Category",

        candidate_bio: "Tell us about your self",

        insta_handle: "Please enter your Insta Handle",

			}, submitHandler: function(form) {

        $('.loading').show();

        var form = $('form')[0]; // You need to use standard javascript object here

        var formData = new FormData(form);

        formData.append('profile_img', $('input[type=file]')[0].files[0]);

                    $.ajax({

                      url: "https://arabasiastarz.com/home/form_submit_ajax",

                      data: formData,

                      type: 'POST',

                      contentType: false, // NEEDED, DON'T OMIT THIS (requires jQuery 1.6+)

                      processData: false, // NEEDED, DON'T OMIT THIS 

                    success: function(result){

                       console.log(result);

                       $('#successModal').modal('toggle');

                       $('#registeration_form').trigger("reset");

                       $('.loading').hide();

                    }});

          }

        }); 

      }); 



// ------------------ cloudanary -----------------------







var myWidget = cloudinary.createUploadWidget({

    sources: [

        "local"

    ],

    clientAllowedFormats: ["mp4","mov","wmv","flv","avi","webM","mkv"],

    maxFileSize: 26214400,

    maxVideoFileSize: 26214400,

    showAdvancedOptions: false,

    cropping: false,

    multiple: false,

    defaultSource: "local",

    styles: {

        palette: {

            window: "#FFFFFF",

            windowBorder: "#90A0B3",

            tabIcon: "#0078FF",

            menuIcons: "#5A616A",

            textDark: "#000000",

            textLight: "#FFFFFF",

            link: "#0078FF",

            action: "#FF620C",

            inactiveTabIcon: "#0E2F5A",

            error: "#F44235",

            inProgress: "#0078FF",

            complete: "#20B832",

            sourceBg: "#E4EBF1"

        },

        fonts: {

            default: {

                active: true

            }

        }

    },

  cloudName: 'arabasiastarz', 

  uploadPreset: 'xrzhejct'}, (error, result) => {

    console.log(error);

    console.log(result);

    if (!error && result && result.event === "success") { 

           $('#video_url').val(result.info.secure_url);

    }

  }

)



document.getElementById("upload_widget").addEventListener("click", function(){

    myWidget.open();

  }, false);


  

  $('input:radio[name="category"]').change(

    function(){

        if ($(this).is(':checked') && $(this).val() == 'Entertainment') {

            $('#sub_category').show();

            $('#sub_category').attr('required','required');

            // console.log('yes');

        }else{

          $('#sub_category').hide();

          $('#sub_category').attr('required',false);

        }

    });

  




</script>

